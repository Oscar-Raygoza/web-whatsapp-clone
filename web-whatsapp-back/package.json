{
  "name": "web-whatsapp-back",
  "version": "1.0.0",
  "description": "Clone Web WhatsApp",
  "main": "./dist/server.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -e ts --exec \"npm run build && npm run start\"",
    "dev2": "cross-env NODE_ENV=development ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "lint": "eslint src/**/*",
    "precommit": "npm run lint",
    "prepush": "npm run build",
    "compile": "tsc && node dist/server.js",
    "start": "nodemon -e ts  --exec \"npm run compile\"",
    "clean": "rm -rf build",
    "ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oscar-Raygoza/web-whatsapp-clone.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oscar-Raygoza/web-whatsapp-clone/issues"
  },
  "homepage": "https://github.com/Oscar-Raygoza/web-whatsapp-clone#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "helmet": "^4.1.0",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.7",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
